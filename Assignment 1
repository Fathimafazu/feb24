lis=[]
Number = int(input("How many elements in list :- "))
count=0 
for i in range(1, Number + 1):
    value = int(input("enter the Value Element :- " ))
    lis.append(value)
for j in lis:
    if(int(j)%2==0):
        count=count+1 
print("the count is",count)
def sum(a, b):
    return (a + b)


